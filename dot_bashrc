source $HOME/.bash/profile.bash

source $HOME/.bash/settings.bash

source $HOME/.bash/aliases.bash

source $HOME/.bash/completion.bash

source $HOME/.bash/paths.bash

source $HOME/.bash/functions.bash

if [ $COMPUTER_TYPE == "desktop" ]; then
  source $HOME/.bash/nix.bash
fi

source $HOME/.bash/devenvs.bash

source $HOME/.bash/machine.bash
